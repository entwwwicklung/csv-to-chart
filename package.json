{
  "name": "csv-to-chart",
  "version": "1.0.0",
  "description": "Calculate CSV report and present it in a bar chart",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "keywords": [
    "csv",
    "report",
    "chart",
    "calculate"
  ],
  "author": "Luka Siljak",
  "license": "ISC",
  "devDependencies": {
    "electron": "^12.0.4",
    "electron-packager": "^15.2.0"
  },
  "build": {
    "productName": "CSV to chart",
    "appId": "csv.to.chart",
    "extraResources": "engine/dist/calculate_csv",
    "win": {
      "target": "portable",
      "icon": "assets/icons/icon.ico"
    }
  },
  "portable": {
    "artifactName": "csv-to-chart.exe"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "chart.js": "^2.9.3",
    "chartjs-plugin-datalabels": "^0.7.0",
    "electron-is-dev": "^1.1.0",
    "npm-font-open-sans": "^1.1.0",
    "sweetalert": "^2.1.2"
  }
}
